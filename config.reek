"app":
  Attribute:
    enabled: false
  IrresponsibleModule:
    enabled: false
  DuplicateMethodCall:
    max_calls: 2
  PrimaDonnaMethod:
    enabled: false
  TooManyStatements:
    max_statements: 30
  NestedIterators:
    max_allowed_nesting: 2
exclude_paths:
  - vendor
  - db
  - bin
  - spec
